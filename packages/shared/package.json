{
  "name": "aidk-shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for AIDK",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "agent",
    "types"
  ],
  "author": "Ryan Lindgren",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rlindgren/aidk.git",
    "directory": "packages/shared"
  },
  "bugs": {
    "url": "https://github.com/rlindgren/aidk/issues"
  },
  "homepage": "https://github.com/rlindgren/aidk#readme",
  "exports": {
    ".": "./src/index.ts",
    "./blocks": "./src/blocks.ts",
    "./block-types": "./src/block-types.ts",
    "./tools": "./src/tools.ts",
    "./input": "./src/input.ts",
    "./messages": "./src/messages.ts",
    "./streaming": "./src/streaming.ts",
    "./models": "./src/models.ts",
    "./timeline": "./src/timeline.ts",
    "./errors": "./src/errors.ts",
    "./identity": "./src/identity.ts",
    "./testing": "./src/testing/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./blocks": {
        "types": "./dist/blocks.d.ts",
        "import": "./dist/blocks.js"
      },
      "./block-types": {
        "types": "./dist/block-types.d.ts",
        "import": "./dist/block-types.js"
      },
      "./tools": {
        "types": "./dist/tools.d.ts",
        "import": "./dist/tools.js"
      },
      "./input": {
        "types": "./dist/input.d.ts",
        "import": "./dist/input.js"
      },
      "./messages": {
        "types": "./dist/messages.d.ts",
        "import": "./dist/messages.js"
      },
      "./streaming": {
        "types": "./dist/streaming.d.ts",
        "import": "./dist/streaming.js"
      },
      "./models": {
        "types": "./dist/models.d.ts",
        "import": "./dist/models.js"
      },
      "./timeline": {
        "types": "./dist/timeline.d.ts",
        "import": "./dist/timeline.js"
      },
      "./errors": {
        "types": "./dist/errors.d.ts",
        "import": "./dist/errors.js"
      },
      "./identity": {
        "types": "./dist/identity.d.ts",
        "import": "./dist/identity.js"
      },
      "./testing": {
        "types": "./dist/testing/index.d.ts",
        "import": "./dist/testing/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch"
  }
}
