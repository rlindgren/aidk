{
  "name": "aidk",
  "version": "0.1.0",
  "description": "React-inspired JSX framework for building AI agents",
  "keywords": [
    "agent",
    "ai",
    "anthropic",
    "google",
    "jsx",
    "llm",
    "openai"
  ],
  "homepage": "https://github.com/rlindgren/aidk#readme",
  "bugs": {
    "url": "https://github.com/rlindgren/aidk/issues"
  },
  "license": "ISC",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rlindgren/aidk.git",
    "directory": "packages/core"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./jsx-runtime": "./src/jsx/jsx-runtime.ts",
    "./jsx/components": "./src/jsx/components/index.ts",
    "./content": "./src/content/index.ts",
    "./model": "./src/model/index.ts",
    "./channels/*": "./src/channels/*.ts",
    "./tool": "./src/tool/index.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./hooks": "./src/hooks/index.ts",
    "./procedure": "./src/procedure/index.ts",
    "./context": "./src/context/index.ts",
    "./config": "./src/config/index.ts",
    "./compiler/*": "./src/compiler/*.ts",
    "./compiler": "./src/compiler/index.ts",
    "./renderers/*": "./src/renderers/*.ts",
    "./renderers": "./src/renderers/index.ts",
    "./utils/*": "./src/utils/*.ts",
    "./utils": "./src/utils/index.ts",
    "./state/*": "./src/state/*.ts",
    "./state": "./src/state/index.ts",
    "./engine": "./src/engine/index.ts",
    "./engine/*": "./src/engine/*.ts",
    "./com": "./src/com/index.ts",
    "./com/*": "./src/com/*.ts",
    "./component/*": "./src/component/*.ts",
    "./component": "./src/component/index.ts",
    "./client/*": "./src/client/*.ts",
    "./client": "./src/client/index.ts",
    "./mcp/*": "./src/mcp/*.ts",
    "./mcp": "./src/mcp/index.ts",
    "./*": "./src/*.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./jsx-runtime": {
        "types": "./dist/jsx/jsx-runtime.d.ts",
        "import": "./dist/jsx/jsx-runtime.js"
      },
      "./jsx/components": {
        "types": "./dist/jsx/components/index.d.ts",
        "import": "./dist/jsx/components/index.js"
      },
      "./content": {
        "types": "./dist/content/index.d.ts",
        "import": "./dist/content/index.js"
      },
      "./model": {
        "types": "./dist/model/*.d.ts",
        "import": "./dist/model/index.js"
      },
      "./channels/*": {
        "types": "./dist/channels/*.d.ts",
        "import": "./dist/channels/*.js"
      },
      "./tool": {
        "types": "./dist/tool/*.d.ts",
        "import": "./dist/tool/index.js"
      },
      "./hooks": {
        "types": "./dist/hooks/index.d.ts",
        "import": "./dist/hooks/index.js"
      },
      "./procedure": {
        "types": "./dist/procedure/index.d.ts",
        "import": "./dist/procedure/index.js"
      },
      "./context": {
        "types": "./dist/context/index.d.ts",
        "import": "./dist/context/index.js"
      },
      "./config": {
        "types": "./dist/config/index.d.ts",
        "import": "./dist/config/index.js"
      },
      "./hooks/*": {
        "types": "./dist/hooks/*.d.ts",
        "import": "./dist/hooks/*.js"
      },
      "./utils/*": {
        "types": "./dist/utils/*.d.ts",
        "import": "./dist/utils/*.js"
      },
      "./utils": {
        "types": "./dist/utils/index.d.ts",
        "import": "./dist/utils/index.js"
      },
      "./state/*": {
        "types": "./dist/state/*.d.ts",
        "import": "./dist/state/*.js"
      },
      "./state": {
        "types": "./dist/state/index.d.ts",
        "import": "./dist/state/index.js"
      },
      "./engine": {
        "types": "./dist/engine/index.d.ts",
        "import": "./dist/engine/index.js"
      },
      "./engine/*": {
        "types": "./dist/engine/*.d.ts",
        "import": "./dist/engine/*.js"
      },
      "./com": {
        "types": "./dist/com/index.d.ts",
        "import": "./dist/com/index.js"
      },
      "./com/*": {
        "types": "./dist/com/*.d.ts",
        "import": "./dist/com/*.js"
      },
      "./component/*": {
        "types": "./dist/component/*.d.ts",
        "import": "./dist/component/*.js"
      },
      "./component": {
        "types": "./dist/component/index.d.ts",
        "import": "./dist/component/index.js"
      },
      "./client/*": {
        "types": "./dist/client/*.d.ts",
        "import": "./dist/client/*.js"
      },
      "./client": {
        "types": "./dist/client/index.d.ts",
        "import": "./dist/client/index.js"
      },
      "./renderers/*": {
        "types": "./dist/renderers/*.d.ts",
        "import": "./dist/renderers/*.js"
      },
      "./renderers": {
        "types": "./dist/renderers/index.d.ts",
        "import": "./dist/renderers/index.js"
      },
      "./compiler/*": {
        "types": "./dist/compiler/*.d.ts",
        "import": "./dist/compiler/*.js"
      },
      "./compiler": {
        "types": "./dist/compiler/index.d.ts",
        "import": "./dist/compiler/index.js"
      },
      "./mcp/*": {
        "types": "./dist/mcp/*.d.ts",
        "import": "./dist/mcp/*.js"
      },
      "./mcp": {
        "types": "./dist/mcp/index.d.ts",
        "import": "./dist/mcp/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.0",
    "aidk-kernel": "workspace:*",
    "aidk-shared": "workspace:*",
    "zod": "^4.2.1"
  }
}
