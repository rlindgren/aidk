{
  "name": "aidk",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": ["dist"],
  "exports": {
    ".": "./src/index.ts",
    "./jsx-runtime": "./src/jsx/jsx-runtime.ts",
    "./jsx/components": "./src/jsx/components/index.ts",
    "./content": "./src/content.ts",
    "./model": "./src/model/index.ts",
    "./channels/*": "./src/channels/*.ts",
    "./tool": "./src/tool/index.ts",
    "./utils/*": "./src/utils/*.ts",
    "./utils": "./src/utils/index.ts",
    "./state/*": "./src/state/*.ts",
    "./state": "./src/state/index.ts",
    "./engine": "./src/engine/index.ts",
    "./engine/*": "./src/engine/*.ts",
    "./com": "./src/com/index.ts",
    "./com/*": "./src/com/*.ts",
    "./*": "./src/*.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./jsx-runtime": {
        "types": "./dist/jsx/jsx-runtime.d.ts",
        "import": "./dist/jsx/jsx-runtime.js"
      },
      "./jsx/components": {
        "types": "./dist/jsx/components/index.d.ts",
        "import": "./dist/jsx/components/index.js"
      },
      "./content": {
        "types": "./dist/content.d.ts",
        "import": "./dist/content.js"
      },
      "./model": {
        "types": "./dist/model/*.d.ts",
        "import": "./dist/model/index.js"
      },
      "./channels/*": {
        "types": "./dist/channels/*.d.ts",
        "import": "./dist/channels/*.js"
      },
      "./tool": {
        "types": "./dist/tool/*.d.ts",
        "import": "./dist/tool/index.js"
      },
      "./utils/*": {
        "types": "./dist/utils/*.d.ts",
        "import": "./dist/utils/*.js"
      },
      "./utils": {
        "types": "./dist/utils/index.d.ts",
        "import": "./dist/utils/index.js"
      },
      "./state/*": {
        "types": "./dist/state/*.d.ts",
        "import": "./dist/state/*.js"
      },
      "./state": {
        "types": "./dist/state/index.d.ts",
        "import": "./dist/state/index.js"
      },
      "./engine": {
        "types": "./dist/engine/index.d.ts",
        "import": "./dist/engine/index.js"
      },
      "./engine/*": {
        "types": "./dist/engine/*.d.ts",
        "import": "./dist/engine/*.js"
      },
      "./com": {
        "types": "./dist/com/index.d.ts",
        "import": "./dist/com/index.js"
      },
      "./com/*": {
        "types": "./dist/com/*.d.ts",
        "import": "./dist/com/*.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "aidk-kernel": "workspace:*",
    "aidk-shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "zod": "^4.1.13"
  }
}