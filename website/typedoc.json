{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "../packages/core/src/engine/index.ts",
    "../packages/core/src/tool/index.ts",
    "../packages/core/src/state/index.ts",
    "../packages/core/src/compiler/index.ts",
    "../packages/core/src/com/index.ts",
    "../packages/core/src/procedure/index.ts",
    "../packages/core/src/context/index.ts",
    "../packages/core/src/content/index.ts",
    "../packages/core/src/config/index.ts",
    "../packages/core/src/component/index.ts",
    "../packages/core/src/client/index.ts",
    "../packages/core/src/utils/index.ts",
    "../packages/core/src/hooks/index.ts",
    "../packages/core/src/renderers/index.ts",
    "../packages/core/src/channels/index.ts",
    "../packages/core/src/mcp/index.ts",
    "../packages/core/src/model/index.ts",
    "../packages/core/src/jsx/index.ts",
    "../packages/core/src/testing/index.ts",
    "../packages/core/src/index.ts",
    "../packages/kernel/src/index.ts",
    "../packages/shared/src/testing/index.ts",
    "../packages/shared/src/index.ts",
    "../packages/client/src/index.ts",
    "../packages/server/src/index.ts",
    "../packages/express/src/index.ts",
    "../packages/nestjs/src/index.ts",
    "../packages/react/src/index.ts",
    "../packages/angular/src/index.ts",
    "../packages/adapters/ai-sdk/src/index.ts",
    "../packages/adapters/openai/src/index.ts",
    "../packages/adapters/google/src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "out": "api",
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "tsconfig": "./tsconfig.typedoc.json",
  "skipErrorChecking": true,
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/*.spec.ts",
    "**/*.test.ts",
    "../example/**"
  ],
  "readme": "none",
  "name": "AIDK API Reference",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "cleanOutputDir": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Engine", "Components", "Tools", "State", "Hooks", "*"],
  "sort": ["kind", "alphabetical"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  "outputFileStrategy": "members",
  "indexFormat": "table",
  "parametersFormat": "table",
  "interfacePropertiesFormat": "table",
  "classPropertiesFormat": "table",
  "enumMembersFormat": "table",
  "typeDeclarationFormat": "table",
  "propertyMembersFormat": "table",
  "expandObjects": true,
  "expandParameters": true,
  "useCodeBlocks": true,
  "useHTMLEncodedBrackets": true,
  "sanitizeComments": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/rlindgren/aidk/blob/master/{path}#L{line}"
}
